{
    "version": 2,
    "public": true,
    "cleanUrls": true,
    "trailingSlash": false,
    "builds": [
        {
            "src": "frontend/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/frontend/$1"
        }
    ]
}