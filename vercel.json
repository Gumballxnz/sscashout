{
    "version": 2,
    "builds": [
        {
            "src": "frontend/**",
            "use": "@vercel/static"
        }
    ],
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "http://51.20.9.165/api/:path*"
        },
        {
            "source": "/sw.js",
            "destination": "/frontend/sw.js"
        },
        {
            "source": "/vapidPublicKey.txt",
            "destination": "/frontend/vapidPublicKey.txt"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/$1"
        }
    ]
}